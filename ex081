valores = []
while True:
    valores.append(int(input("Digite um valor: ")))
    resp = str(input("Quer continuar?[S/N] "))
    if resp in 'Nn':
        break
print("-=" * 30)
print(f'Você digitou {len(valores)} elementos.')
valores.sort(reverse=True)
print(f'Os vlores em ordem decrescente são {valores}.')
if 5 in valores:
    print(f'O numéro 5 foi digitado.')
else:
    print(f'O numéro 5 não foi digitado.')